MNIST 80% with four layers 

-- hyper-parameters 

input_size_x = 14
input_size_y = 14
input_k = 4
rnn_size = 100
hiddenLayer = 4096
output_size = 1
nIndex = 10
n_layers = 1
dropout = 0 
should_tie_weights = 0
lr = 0.001
length = input_size_x * input_size_y
batch_size = 32
rho = length -- sequence length
local mnist = require 'mnist'


nn.Sequential {
  [input -> (1) -> (2) -> (3) -> (4) -> (5) -> output]
  (1): nn.Sequential {
    [input -> (1) -> (2) -> (3) -> output]
    (1): nn.ConcatTable {
      input
        |`-> (1): nn.Sequencer @ nn.GridLSTM
        |`-> (2): nn.Sequential {
        |      [input -> (1) -> (2) -> (3) -> output]
        |      (1): nn.SymmetricTable
        |      (2): nn.Sequencer @ nn.GridLSTM
        |      (3): nn.SymmetricTable
        |    }
        |`-> (3): nn.Sequential {
        |      [input -> (1) -> (2) -> (3) -> (4) -> (5) -> output]
        |      (1): nn.SymmetricTable
        |      (2): nn.ReverseTable
        |      (3): nn.Sequencer @ nn.GridLSTM
        |      (4): nn.ReverseTable
        |      (5): nn.SymmetricTable
        |    }
        |`-> (4): nn.Sequential {
        |      [input -> (1) -> (2) -> (3) -> output]
        |      (1): nn.ReverseTable
        |      (2): nn.Sequencer @ nn.GridLSTM
        |      (3): nn.ReverseTable
        |    }
         ... -> output
    }
    (2): nn.ZipTable
    (3): nn.Sequencer @ nn.Recursor @ nn.CAddTable
  }
  (2): nn.JoinTable
  (3): nn.Linear(19600 -> 4096)
  (4): nn.Linear(4096 -> 10)
  (5): nn.LogSoftMax
}
